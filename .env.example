# OpenAI API Key - Required
OPENAI_API_KEY=your_openai_api_key

# Model Configuration
MODEL_BLUEPRINT_AUTHORING=gpt-4o
MODEL_BLUEPRINT_REVIEWING=gpt-4o
MODEL_SCRIPT_CODING=gpt-4o

# Performance Configuration
AUTONOMOUS_MAX_ITERATIONS=3

# Logging Configuration 
LOG_LEVEL=INFO

# Server Configuration
PORT=8000
HOST=localhost
FRONTEND_URL=http://localhost:5173

# Optional - Uncomment if you want to set model temperature
# MODEL_TEMPERATURE=0.7

# Optional - Uncomment if you want to set model complexity thresholds
# AUTONOMOUS_MIN_COMPLEXITY=0.6
# AUTONOMOUS_MAX_COMPLEXITY=0.8

# ACCESS_TOKENS: JSON string mapping identifiers to bearer tokens
# Example: '{"user_alpha": "token_abc123", "project_beta": "token_xyz456"}'
ACCESS_TOKENS='{"example_user": "example_token_1234"}'

# ADMIN_TOKEN: A secret token for accessing admin endpoints
ADMIN_TOKEN='example_admin_token_5678' 